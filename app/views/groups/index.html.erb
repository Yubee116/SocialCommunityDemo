<div class="w-full">
  <%= render 'groups/group_nav'%>
  <% if notice.present? %>
    <p class="py-2 px-3 bg-green-50 mb-5 text-green-500 font-medium rounded-lg inline-block" id="notice"><%= notice %></p>
  <% end %>
  <% if alert.present? %>
    <div class="py-2 px-3 bg-red-50 mb-5 text-red-500 font-medium rounded-lg inline-block" id="notice"><%= alert %></div>
  <% end %> 


  <div id="groups" class="min-w-full">
    <%= render @groups %>
  </div>
</div>
